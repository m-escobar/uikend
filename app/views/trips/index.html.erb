<h2>Pacotes de viagens</h1>
<% @trips.each do |trip| %>
  <h3><%= trip.name %></h2>
  <h4><%= trip.description %></h4>
  <h5><%= trip.place %></h5>
  <h5><%= trip.start %> - <%= trip.end %></h5>
  <p><%= trip.hotel %></p>
  <p><%= trip.schedule %></p>
  <p><%= trip.price %></p>
  <p><%= user.email %></p>
<% end %>
